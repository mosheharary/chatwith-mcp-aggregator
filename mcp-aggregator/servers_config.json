{
  "mcpServers": {
    "neo4j": {
      "command": "uvx",
      "args": [ "mcp-neo4j-cypher@0.3.0", "--transport", "stdio"  ],
      "env": {
      	"NEO4J_URI": "bolt://host.docker.internal:7687",
      	"NEO4J_USERNAME": "neo4j",
      	"NEO4J_PASSWORD": "password",
      	"NEO4J_DATABASE": "neo4j"
      }
    }
  }
}
